{
    "name": "gosuperscript/schema-lookup",
    "description": "A PHP library for querying CSV/TSV files with streaming, dynamic filtering, aggregate functions, and range-based banding.",
    "type": "library",
    "license": "proprietary",
    "keywords": [
        "php",
        "csv",
        "tsv",
        "lookup",
        "aggregation",
        "streaming",
        "filtering"
    ],
    "require": {
        "php": "^8.4",
        "gosuperscript/monads": "^1.0.0",
        "gosuperscript/schema": "*",
        "league/csv": "^9.27.0"
    },
    "require-dev": {
        "infection/infection": "^0.29.14",
        "laravel/pint": "^1.22",
        "phpbench/phpbench": "^1.3",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.0",
        "robiningelbrecht/phpunit-coverage-tools": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Superscript\\Schema\\Lookup\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Superscript\\Schema\\Lookup\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test:types": "vendor/bin/phpstan analyse",
        "test:unit": "vendor/bin/phpunit -d --min-coverage=100",
        "test:infection": "vendor/bin/infection --threads=max --show-mutations",
        "test": [
            "@test:types",
            "@test:unit",
            "@test:infection"
        ],
        "bench": "vendor/bin/phpbench run --report=default",
        "bench:aggregate": "vendor/bin/phpbench run --group=aggregate --report=default",
        "bench:memory": "vendor/bin/phpbench run --group=memory --report=default"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        },
        "platform": {
            "php": "8.4.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://mannum.github.io/satis"
        }
    ]
}
